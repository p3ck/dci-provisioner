#!/bin/sh
set -e
if [ "$power_mode" == interrupt ] ; then
    echo "interrupt not supported by apc_snmp" >&2
    exit 1
fi
# Handle one or more plug numbers
# power_id must be a whitespace delimited list
for plug in $power_id; do
    ssh -i /etc/dci-rhel-agent/secrets/id_rsa -o UserKnownHostsFile=/dev/null -o "StrictHostKeyChecking no" "$JUMPHOST" fence_apc_snmp -a "$power_address" -n $plug -o "$power_mode"
done
